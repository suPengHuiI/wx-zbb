<!--index.wxml-->
<view class="index">
  <view class='nav'>
    <view catchtap='changeNav' data-index="{{index}}" wx:for="{{nav}}" wx:key="index">
      <text class="{{active==index?'active':''}}">{{item.item}}</text>
    </view>
    <view class='search'>
      <image src='/images/search.png'></image>
    </view>
  </view>
  <view style='width:100%;height:100rpx;'></view>
  <postings />
</view>